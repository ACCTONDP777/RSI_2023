/*
 * Skin: Navy
 * ------------
 */
.skin-navy-light .main-header .navbar {
  background-color: #001f3f;
}
.skin-navy-light .main-header .navbar .nav > li > a {
  color: #ffffff;
}
.skin-navy-light .main-header .navbar .nav > li > a:hover,
.skin-navy-light .main-header .navbar .nav > li > a:active,
.skin-navy-light .main-header .navbar .nav > li > a:focus,
.skin-navy-light .main-header .navbar .nav .open > a,
.skin-navy-light .main-header .navbar .nav .open > a:hover,
.skin-navy-light .main-header .navbar .nav .open > a:focus,
.skin-navy-light .main-header .navbar .nav > .active > a {
  background: rgba(255, 255, 255, 0.2);
  color: #f6f6f6;
}
.skin-navy-light .main-header .navbar .sidebar-toggle {
  color: #ffffff;
}
.skin-navy-light .main-header .navbar .sidebar-toggle:hover {
  color: #f6f6f6;
  background: rgba(255, 255, 255, 0.2);
}
.skin-navy-light .main-header .navbar .sidebar-toggle {
  color: #fff;
}
@media (max-width: 767px) {
  .skin-navy-light .main-header .navbar .dropdown-menu li.divider {
    background-color: rgba(255, 255, 255, 0.1);
  }
  .skin-navy-light .main-header .navbar .dropdown-menu li a {
    color: #fff;
  }
  .skin-navy-light .main-header .navbar .dropdown-menu li a:hover {
    background: #1f6ebf;
  }
}
.skin-navy-light .main-header .logo {
  background-color: #001f3f;
  color: #ffffff;
  border-bottom: 0 solid transparent;
}
.skin-navy-light .main-header .logo:hover {
  background-color: rgba(255, 255, 255, 0.2);
}
.skin-navy-light .main-header li.user-header {
  background-color: #001f3f;
}
.skin-navy-light .content-header {
  background: transparent;
}
.skin-navy-light .wrapper,
.skin-navy-light .main-sidebar,
.skin-navy-light .left-side {
  background-color: #f9fafc;
}
.skin-navy-light .main-sidebar {
  border-right: 1px solid #d2d6de;
}
.skin-navy-light .user-panel > .info,
.skin-navy-light .user-panel > .info > a {
  color: #444444;
}
.skin-navy-light .sidebar-menu > li {
  -webkit-transition: border-left-color 0.3s ease;
  -o-transition: border-left-color 0.3s ease;
  transition: border-left-color 0.3s ease;
}
.skin-navy-light .sidebar-menu > li.header {
  color: #848484;
  background: #f9fafc;
}
.skin-navy-light .sidebar-menu > li > a {
  border-left: 3px solid transparent;
  font-weight: 600;
}
.skin-navy-light .sidebar-menu > li:hover > a,
.skin-navy-light .sidebar-menu > li.active > a {
  color: #000000;
  background: #f4f4f5;
}
.skin-navy-light .sidebar-menu > li.active {
  border-left-color: #001f3f;
}
.skin-navy-light .sidebar-menu > li.active > a {
  font-weight: 600;
}
.skin-navy-light .sidebar-menu > li > .treeview-menu {
  background: #f4f4f5;
}
.skin-navy-light .sidebar a {
  color: #444444;
}
.skin-navy-light .sidebar a:hover {
  text-decoration: none;
}
.skin-navy-light .sidebar-menu .treeview-menu > li > a {
  color: #777777;
}
.skin-navy-light .sidebar-menu .treeview-menu > li.active > a,
.skin-navy-light .sidebar-menu .treeview-menu > li > a:hover {
  color: #000000;
}
.skin-navy-light .sidebar-menu .treeview-menu > li.active > a {
  font-weight: 600;
}
.skin-navy-light .sidebar-form {
  border-radius: 3px;
  border: 1px solid #d2d6de;
  margin: 10px 10px;
}
.skin-navy-light .sidebar-form input[type="text"],
.skin-navy-light .sidebar-form .btn {
  box-shadow: none;
  background-color: #fff;
  border: 1px solid transparent;
  height: 35px;
}
.skin-navy-light .sidebar-form input[type="text"] {
  color: #666;
  border-top-left-radius: 2px;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
  border-bottom-left-radius: 2px;
}
.skin-navy-light .sidebar-form input[type="text"]:focus,
.skin-navy-light .sidebar-form input[type="text"]:focus + .input-group-btn .btn {
  background-color: #fff;
  color: #666;
}
.skin-navy-light .sidebar-form input[type="text"]:focus + .input-group-btn .btn {
  border-left-color: #fff;
}
.skin-navy-light .sidebar-form .btn {
  color: #999;
  border-top-left-radius: 0;
  border-top-right-radius: 2px;
  border-bottom-right-radius: 2px;
  border-bottom-left-radius: 0;
}
@media (min-width: 768px) {
  .skin-navy-light.sidebar-mini.sidebar-collapse .sidebar-menu > li > .treeview-menu {
    border-left: 1px solid #d2d6de;
  }
}

.skin-navy-light a{
    color: #001f3f;
}

.skin-navy-light a:hover{
    color: #1f6ebf;
}


.skin-navy-light .main-header{
    background-color: #001f3f; 
}

.skin-navy-light .sidebar-menu > li.active {
  border-left: 3px solid #1f6ebf;
}

.skin-navy-light .btn-primary{
    background: #001f3f;
    border-color: #1f6ebf;
}

.skin-navy-light .btn-primary:hover,
.skin-navy-light .btn-primary:active,
.skin-navy-light .btn-primary.hover{
    background: #1f6ebf;
}

.skin-navy-light.login-page .login-box{
    background: #001f3f;
}

.skin-navy-light.login-page{
    background: #bbc1c3;
}

.skin-navy-light .pagination > .active > a, 
.skin-navy-light .pagination > .active > a:focus,
.skin-navy-light .pagination > .active > a:hover,
.skin-navy-light .pagination > .active > span,
.skin-navy-light .pagination > .active > span:focus,
.skin-navy-light .pagination > .active > span:hover{
    background: #001f3f;
}

.skin-navy-light .info-box .progress .progress-bar{
    background: #1f6ebf;
}